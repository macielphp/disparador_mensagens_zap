{
    "manifest_version": 3,
    "name": "Disparador de Mensagens WhatsApp",
    "version": "1.0",
    "description": "Extens√£o para enviar mensagens no WhatsApp Web.",
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "host_permissions":[
        "https://web.whatsapp.com/*"
    ],
    "content_scripts": {
        "content_scripts": [
            {
                "matches":["https://web.whatsapp.com/*"],
                "js": ["content.js"]
            }
        ]
    }
}